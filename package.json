{
  "name": "vue-form-container",
  "version": "1.0.0",
  "description": "A Provider Component that encapsulate your forms and handle their states and validations.",
  "scripts": {
    "build": "NODE_ENV=production bili src/FormContainer.vue --formats es,cjs,umd --plugins vue --banner --js buble --moduleName VueFormContainer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/vue-form-container.git"
  },
  "files": ["dist/"],
  "cdn": "dist/vue-form-container.js",
  "main": "dist/vue-form-container.cjs.js",
  "unpkg": "dist/vue-form-container.js",
  "module": "dist/vue-form-container.es.js",
  "jsdelivr": "dist/vue-form-container.js",
  "umd:main": "dist/vue-form-container.js",
  "keywords": [
