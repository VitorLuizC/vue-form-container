  "bugs": {
    "url": "https://github.com/VitorLuizC/vue-form-container/issues"
  },
  "homepage": "https://github.com/VitorLuizC/vue-form-container#readme",
  "devDependencies": {
    "bili": "^3.4.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "dependencies": {
    "valite": "^0.7.0"
